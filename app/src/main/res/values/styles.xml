<resources>

    <!-- Base application theme. -->
    <style content="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item content="colorPrimary">@color/colorPrimary</item>
        <item content="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item content="colorAccent">@color/colorAccent</item>
    </style>

    <style content="AppTheme.NoActionBar">
        <item content="windowActionBar">false</item>
        <item content="windowNoTitle">true</item>
    </style>

    <style content="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style content="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
