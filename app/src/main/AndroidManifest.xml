<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobileeye.mobileeye">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:content=".Activities.MainActivity">
            <intent-filter>
                <action android:content="android.intent.action.MAIN" />

                <category android:content="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:content=".Activities.MenuActivity" />

        <uses-permission android:content="android.permission.RECORD_AUDIO" />

        <activity android:content=".Activities.NavigationActivity" />
        <activity android:content=".Activities.VoiceNotesActivity" />
        <activity android:content=".Activities.ObjectRecognitionActivity"></activity>
    </application>

</manifest>